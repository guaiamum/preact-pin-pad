{
    "presets": [
      ["@babel/preset-env", { "modules": false }]
    ],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", { "pragma": "h" }],
        ["@babel/plugin-proposal-class-properties"],
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
  }
